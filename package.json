{
	"name": "next",
	"version": "0.1.0",
	"private": true,
	"pnpm": {
		"onlyBuiltDependencies": [
			"lefthook"
		]
	},
	"scripts": {
		"dev": "next dev --webpack",
		"build": "next build --webpack",
		"start": "next start",
		"test": "vitest run",
		"lint": "oxlint src",
		"lint:fix": "oxlint --fix src",
		"format": "oxfmt src",
		"format:check": "oxfmt --check src",
		"patch:next": "node scripts/patch-next-fast-set-immediate.mjs",
		"predeploy": "pnpm run patch:next",
		"preupload": "pnpm run patch:next",
		"prepreview": "pnpm run patch:next",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply habu_db --local",
		"db:migrate:remote": "wrangler d1 migrations apply habu_db --remote"
	},
	"dependencies": {
		"@ai-sdk/openai": "^3.0.0",
		"@ai-sdk/react": "^3.0.28",
		"@better-auth/oauth-provider": "^1.4.11",
		"@cloudflare/workers-types": "^4.20251121.0",
		"@mastra/core": "^0.24.6",
		"@modelcontextprotocol/sdk": "^1.25.2",
		"@opennextjs/cloudflare": "^1.14.8",
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@serwist/next": "^9.2.3",
		"@tanstack/react-virtual": "^3.13.13",
		"@types/crypto-js": "^4.2.2",
		"ai": "^6.0.0",
		"better-auth": "^1.4.11",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"crypto-js": "^4.2.0",
		"dexie": "^4.2.1",
		"dexie-react-hooks": "^4.2.0",
		"drizzle-orm": "^0.45.0",
		"lucide-react": "^0.562.0",
		"mastra": "^0.18.6",
		"next": "16.1.1",
		"next-themes": "^0.4.6",
		"oauth-1.0a": "^2.2.6",
		"openai": "^6.9.1",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"serwist": "^9.2.3",
		"sonner": "^2.0.7",
		"streamdown": "^2.0.0",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "0.12.3",
		"@tailwindcss/postcss": "4.1.18",
		"@types/node": "24.10.8",
		"@types/react": "19.2.8",
		"@types/react-dom": "19.2.3",
		"@vitest/runner": "3.2.4",
		"@vitest/snapshot": "3.2.4",
		"babel-plugin-react-compiler": "1.0.0",
		"drizzle-kit": "0.31.8",
		"lefthook": "2.0.15",
		"oxfmt": "0.24.0",
		"oxlint": "1.39.0",
		"sharp": "0.34.5",
		"tailwindcss": "4.1.18",
		"tw-animate-css": "1.4.0",
		"typescript": "5.9.3",
		"vitest": "3.2.4",
		"wrangler": "4.59.1"
	}
}
